{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "DISCORD_TOKEN": "@discord_token",
    "WEBHOOK_URL": "@webhook_url",
    "TARGET_BASE": "@target_base"
  }
}
